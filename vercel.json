{
  "version": 2,
  "builds": [
    {
      "src": "app/api/chat/agent.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/chat/query",
      "dest": "app/api/chat/agent.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "app/api/chat/agent.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 